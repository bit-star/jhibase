


entity MpHotspot{
	name String,

	imageUrl String,

	pathUrl String ,

	addTime Instant,

	orderNum Long,
  
	remark String
}

entity FmpSubCompany{
	moreTodoUrl String
}





relationship OneToMany {
  FmpSubCompany to MpHotspot,
}


service all with serviceClass

// Set an angular suffix
angularSuffix * with mp
